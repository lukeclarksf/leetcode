   "source": [
    "def maxSubArray(nums: list[int]) -> int:\n",
    "    \n",
    "    current_max = nums[0]\n",
    "    global_max = nums[0]\n",
    "    \n",
    "    for i in range(1, len(nums)):\n",
    "        current_max = max(nums[i], current_max + nums[i])\n",
    "        global_max = max(global_max, current_max)\n",
    "        \n",
    "    return global_max"
   ]
